{
  "name": "gzhegow/eventman",
  "type": "library",
  
  "authors": [
    {
      "name": "gzhegow",
      "email": "6562680@gmail.com"
    }
  ],
  
  "minimum-stability": "dev",
  "prefer-stable": true,
  
  "require": {
    "php": "^7.2|^8.0",
    
    "psr/cache": "1.0.1",
    "psr/container": "1.0.0",
    "symfony/var-dumper": "^5.4"
  },
  
  "autoload": {
    "psr-4": {
      "Gzhegow\\Di\\": "src/"
    },
    "files": [
      "helpers/assert.php",
      "helpers/di.php",
      "helpers/php.php"
    ]
  },
  
  "config": {
    "optimize-autoloader": true,
    
    "sort-packages": false
  },
  "require-dev": {
    "symfony/cache": "^5.4"
  }
}
